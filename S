                    local localChar = workspace.Game.Players:FindFirstChild(PlayerName)
                    if localChar then
                        local state = GetPlayerState(PlayerName)
                        if state == "Downed" then
                            local args = {
                                [1] = true
                            }
                            ReplicatedStorage.Events.Player.ChangePlayerMode:FireServer(unpack(args))
                            wait(3)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
