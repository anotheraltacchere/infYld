    autoShoot = false,
    shootButton = "MouseButton1",
    shootDelay = 0.01,
    externalTargetIsModel = true,
    lastTargetTime = 0,
    targetTimeout = 0.5,
    ignoreWalls = false,
    autoShootTolerance = 15, -- Increased tolerance for auto-shoot detection
    lastShootTime = 0,
    shootCooldown = 0.1
}

-- Team settings
local teamSettings = {
    teamCheckEnabled = false,
    targetEnemiesOnly = true,
    espTeammates = false,
    espEnemies = true,
    localPlayerTeam = nil,
    teamColors = {},
    teamWhitelist = {},
    playerWhitelist = {}
}

-- ShiftLock settings
