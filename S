local coreGui = game:GetService("CoreGui")
local commandBar = coreGui:FindFirstChild("ScreenGui")

if commandBar then
    local bar = commandBar:FindFirstChild("CommandBar")
    if bar then
        bar:Destroy()
        print("CommandBar destroyed.")
