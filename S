    Name = "Aura Settings"
})

MainTab:AddToggle({
    Name = "Enable/Disable aura tool disabler",
    Default = getgenv().Settings.AuraDisabler,
    Color = Color3.fromRGB(0, 0, 255),
    Callback = function(Value)
        getgenv().Settings.AuraDisabler = Value
        setupAuraDisabler()
        setupAuraVisualization()
    end
})

