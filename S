                    end
                else
                    OrionLib:MakeNotification({
                        Name = "Error",
                        Content = "Could not find player's aura!",
                        Image = "rbxassetid://4483345998",
                        Time = 3
                    })
                end
            end)
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Please select a player first!",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        end
    end
})

