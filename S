AimbotTab:AddToggle({
    Name = "Show FOV Circle",
    Default = true,
    Callback = function(Value)
        aimbotSettings.showFOVCircle = Value
        if fovCircle then
            fovCircle.Visible = Value and aimbotSettings.enabled and not (aimbotSettings.fov360 or aimbotSettings.fov360True)
        end
    end
})

