    Name = "Aura visualization",
    Default = getgenv().Settings.ShowAuraVisualization,
    Color = Color3.fromRGB(0, 0, 255),
    Callback = function(Value)
        getgenv().Settings.ShowAuraVisualization = Value
        setupAuraVisualization()
    end
})

