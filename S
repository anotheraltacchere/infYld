-- Highlight storage
local highlights = {}
local nameLabels = {}
local healthLabels = {}
local distanceLabels = {}

-- ESP states
local espEnabled = false
local playerEspEnabled = false
local noEspPlayers = false

-- External targets
local externalTargets = {}
local externalTargetLabels = {}
